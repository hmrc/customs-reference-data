{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Generated schema for Root",
  "type": "object",
  "properties": {
    "messageInformation": {
      "type": "object",
      "properties": {
        "messageID": {
          "type": "string",
          "pattern": "[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}"
        },
        "snapshotDate": {
          "type": "string",
          "pattern": "\\d{4}-[01]\\d-[0-3]\\d"
        }
      },
      "required": [
        "messageID",
        "snapshotDate"
      ]
    },
    "lists": {
      "type": "object",
      "properties": {
        "customsOffices": {
          "type": "object",
          "properties": {
            "listName": {
              "type": "string",
              "minLength": 0,
              "maxLength": 4000
            },
            "listEntries": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "state": {
                    "type": "string",
                    "enum": [
                      "valid",
                      "invalid"
                    ]
                  },
                  "activeFrom": {
                    "type": "string",
                    "pattern": "\\d{4}-[01]\\d-[0-3]\\d"
                  },
                  "id": {
                    "type": "string",
                    "pattern": "[A-Z]{2}[0-9]{6}"
                  },
                  "name": {
                    "type": "string",
                    "minLength": 0,
                    "maxLength": 4000
                  },
                  "LanguageCode": {
                    "type": "string",
                    "minLength": 0,
                    "maxLength": 4000
                  },
                  "countryId": {
                    "type": "string",
                    "minLength": 0,
                    "maxLength": 4000
                  },
                  "phoneNumber": {
                    "type": "string",
                    "minLength": 0,
                    "maxLength": 4000
                  },
                  "eMailAddress": {
                    "type": "string",
                    "minLength": 0,
                    "maxLength": 4000
                  },
                  "roles": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "properties": {
                        "role": {
                          "type": "string",
                          "minLength": 0,
                          "maxLength": 4000
                        }
                      },
                      "required": [
                        "role"
                      ]
                    }
                  }
                },
                "required": [
                  "state",
                  "activeFrom",
                  "id",
                  "name",
                  "LanguageCode",
                  "countryId",
                  "roles"
                ]
              }
            }
          },
          "required": [
            "listName",
            "listEntries"
          ]
        }
      },
      "required": [
        "customsOffices"
      ]
    }
  },
  "required": [
    "messageInformation",
    "lists"
  ]
}