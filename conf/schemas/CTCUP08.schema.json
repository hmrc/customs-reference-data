{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$ref": "#/definitions/object_06",
  "definitions": {
    "object_01": {
      "type": "object",
      "required": [
        "seasonCode",
        "seasonStartDate",
        "seasonEndDate",
        "customsOfficeTimetableLine",
        "customsOfficeRoleTrafficCompetence"
      ],
      "additionalProperties": false,
      "properties": {
        "seasonCode": {
          "type": "number",
          "description": "The Season code",
          "examples": [
            1
          ]
        },
        "seasonName": {
          "type": "string",
          "description": "The Season name",
          "examples": [
            "ALL YEAR"
          ]
        },
        "seasonStartDate": {
          "type": "string",
          "pattern": "^([12]\\d{3}(0[1-9]|1[0-2])(0[1-9]|[12]\\d|3[01]))$",
          "description": "The Season start date",
          "examples": [
            "20180101"
          ]
        },
        "seasonEndDate": {
          "type": "string",
          "pattern": "^([12]\\d{3}(0[1-9]|1[0-2])(0[1-9]|[12]\\d|3[01]))$",
          "description": "The Season end date",
          "examples": [
            "20991231"
          ]
        },
        "customsOfficeTimetableLine": {
          "type": "array",
          "title": "customsOfficeTimetableLine",
          "description": "The Customs Office Timetable line items",
          "items": {
            "type": "object",
            "required": [
              "dayInTheWeekBeginDay",
              "openingHoursTimeFirstPeriodFrom",
              "openingHoursTimeFirstPeriodTo",
              "dayInTheWeekEndDay"
            ],
            "additionalProperties": false,
            "properties": {
              "dayInTheWeekBeginDay": {
                "type": "number",
                "description": "The starting day in the week",
                "minimum": 1,
                "maximum": 7
              },
              "openingHoursTimeFirstPeriodFrom": {
                "type": "string",
                "pattern": "^([0-1][0-9]|2[0-3])[0-5][0-9]$",
                "description": "The first period opening time",
                "examples": [
                  "0800"
                ]
              },
              "openingHoursTimeFirstPeriodTo": {
                "type": "string",
                "pattern": "^([0-1][0-9]|2[0-3])[0-5][0-9]$",
                "description": "The first period finishing time",
                "examples": [
                  "1700"
                ]
              },
              "openingHoursTimeSecondPeriodFrom": {
                "type": "string",
                "pattern": "^([0-1][0-9]|2[0-3])[0-5][0-9]$",
                "description": "The second period opening time",
                "examples": [
                  "1200"
                ]
              },
              "openingHoursTimeSecondPeriodTo": {
                "type": "string",
                "pattern": "^([0-1][0-9]|2[0-3])[0-5][0-9]$",
                "description": "The second period finishing time",
                "examples": [
                  "1800"
                ]
              },
              "dayInTheWeekEndDay": {
                "type": "number",
                "description": "The finishing day in the week",
                "minimum": 1,
                "maximum": 7
              }
            }
          }
        },
        "customsOfficeRoleTrafficCompetence": {
          "type": "array",
          "title": "customsOfficeRoleTrafficCompetence",
          "description": "The customs office competency",
          "items": {
            "type": "object",
            "required": [
              "role",
              "trafficType"
            ],
            "additionalProperties": false,
            "properties": {
              "role": {
                "type": "string",
                "pattern": "^[A-Z]{3}$",
                "description": "The role",
                "examples": [
                  "AUT"
                ]
              },
              "trafficType": {
                "type": "string",
                "pattern": "^[A-Z]{1,3}$|^N\/A$",
                "description": "The traffic type",
                "examples": [
                  "R"
                ]
              }
            }
          }
        }
      }
    },
    "object_02": {
      "type": "object",
      "required": [
        "state",
        "activeFrom",
        "referenceNumber",
        "countryCode",
        "postalCode",
        "phoneNumber",
        "traderDedicated",
        "customsOfficeDetails",
        "customsOfficeTimetable"
      ],
      "additionalProperties": false,
      "properties": {
        "state": {
          "type": "string",
          "pattern": "^(valid|invalid)$",
          "description": "Valid or Invalid entry",
          "examples": [
            "valid"
          ]
        },
        "activeFrom": {
          "type": "string",
          "pattern": "^([12]\\d{3}-(0[1-9]|1[0-2])-(0[1-9]|[12]\\d|3[01]))$",
          "description": "The date the entry was active",
          "examples": [
            "2019-01-01"
          ]
        },
        "referenceNumber": {
          "type": "string",
          "pattern": "^[A-Z]{2}[0-9]{6}$",
          "description": "The Customs Office List Reference Number",
          "examples": [
            "AD000003"
          ]
        },
        "referenceNumberMainOffice": {
          "type": "string",
          "pattern": "^[A-Z]{2}[0-9]{6}$",
          "description": "The Main Office Reference Number",
          "examples": [
            "AD000003"
          ]
        },
        "referenceNumberHigherAuthority": {
          "type": "string",
          "pattern": "^[A-Z]{2}[0-9]{6}$",
          "description": "The Higher Office Reference Number",
          "examples": [
            "AD000003"
          ]
        },
        "referenceNumberCompetentAuthorityOfEnquiry": {
          "type": "string",
          "pattern": "^[A-Z]{2}[0-9]{6}$",
          "description": "The Competent Authority of Enquiry Reference Number",
          "examples": [
            "AD000003"
          ]
        },
        "referenceNumberCompetentAuthorityOfRecovery": {
          "type": "string",
          "pattern": "^[A-Z]{2}[0-9]{6}$",
          "description": "The Competent Authority of Recovery Reference Number",
          "examples": [
            "AD000003"
          ]
        },
        "countryCode": {
          "type": "string",
          "pattern": "^[A-Z]{2}$",
          "description": "The Country Code",
          "examples": [
            "AD"
          ]
        },
        "unLocodeId": {
          "type": "string",
          "pattern": "^[A-Z]{3}$",
          "description": "The UN Locode ID",
          "examples": [
            "ALV"
          ]
        },
        "nctsEntryDate": {
          "type": "string",
          "pattern": "^([12]\\d{3}(0[1-9]|1[0-2])(0[1-9]|[12]\\d|3[01]))$",
          "description": "The NCTS Entry Date",
          "examples": [
            "20070614"
          ]
        },
        "nearestOffice": {
          "type": "string",
          "pattern": "^[A-Z]{2}[0-9]{6}$",
          "description": "The Nearest Customs Office",
          "examples": [
            "CH002621"
          ]
        },
        "postalCode": {
          "type": "string",
          "description": "The Post Code",
          "examples": [
            "AD700"
          ]
        },
        "phoneNumber": {
          "type": "string",
          "description": "The Phone Number",
          "examples": [
            "+ (376) 879900"
          ]
        },
        "faxNumber": {
          "type": "string",
          "description": "The FAX Number",
          "examples": [
            "+ (376) 860360"
          ]
        },
        "eMailAddress": {
          "type": "string",
          "description": "The Email address",
          "format": "email"
        },
        "geoInfoCode": {
          "type": "string",
          "pattern": "^[A-Z]{2}\/[A-Z]{2}$|^[A-Z]{1,3}$",
          "description": "The GEO Information Code",
          "examples": [
            "Q"
          ]
        },
        "traderDedicated": {
          "type": "number",
          "minimum": 0,
          "maximum": 1,
          "description": "Flag to indicate if the Customs office is dedicated to traders",
          "examples": [
            1
          ]
        },
        "regionCode": {
          "type": "string",
          "pattern": "^[A-Z]{2,3}$",
          "description": "The Country Region code",
          "examples": [
            "BW"
          ]
        },
        "telexNumber": {
          "type": "string",
          "description": "The Telex number",
          "examples": [
            "+358005671"
          ]
        },
        "specificNotesCode": {
          "type": "string",
          "pattern": "^SN[0-9]{4}$",
          "description": "The Specific notes code",
          "examples": [
            "SN0002"
          ]
        },
        "dedicatedTrader": {
          "type": "array",
          "title": "dedicatedTrader",
          "description": "Dedicated Trader details in different languages",
          "items": {
            "type": "object",
            "required": [
              "languageCode",
              "name"
            ],
            "additionalProperties": false,
            "properties": {
              "languageCode": {
                "type": "string",
                "pattern": "^[A-Z]{2}$",
                "description": "The language code",
                "examples": [
                  "EN"
                ]
              },
              "name": {
                "type": "string",
                "description": "The name",
                "examples": [
                  "H M Revenue & Customs"
                ]
              }
            }
          }
        },
        "customsOfficeDetails": {
          "type": "array",
          "title": "customsOfficeDetails",
          "description": "Further Customs Office Details",
          "items": {
            "type": "object",
            "required": [
              "languageCode",
              "customsOfficeUsualName",
              "streetAndNumber",
              "city",
              "prefixSuffixFlag",
              "spaceToAdd"
            ],
            "additionalProperties": false,
            "properties": {
              "languageCode": {
                "type": "string",
                "pattern": "^[A-Z]{2}$",
                "description": "The language code",
                "examples": [
                  "EN"
                ]
              },
              "customsOfficeUsualName": {
                "type": "string",
                "description": "The usual/familar name of the customs office",
                "examples": [
                  "CENTRAL CUSTOMS OFFICE"
                ]
              },
              "streetAndNumber": {
                "type": "string",
                "description": "The Street name and number",
                "examples": [
                  "AVINGUDA FITER I ROSSELL, 2"
                ]
              },
              "city": {
                "type": "string",
                "description": "The city",
                "examples": [
                  "ESCALDES - ENGORDANY"
                ]
              },
              "prefixSuffixFlag": {
                "type": "number",
                "minimum": 0,
                "maximum": 1,
                "description": "The Prefix/Suffix flag",
                "examples": [
                  0
                ]
              },
              "prefixSuffixLevel": {
                "type": "string",
                "pattern": "^[A-Z]$",
                "description": "The Prefix/Suffix flag",
                "examples": [
                  "E"
                ]
              },
              "prefixSuffixName": {
                "type": "string",
                "description": "The Prefix/Suffix Name",
                "examples": [
                  "Tulli"
                ]
              },
              "spaceToAdd": {
                "type": "number",
                "minimum": 0,
                "maximum": 1,
                "description": "Space to add indicator",
                "examples": [
                  0
                ]
              }
            }
          }
        },
        "customsOfficeTimetable": {
          "type": "array",
          "title": "customsOfficeTimetable",
          "description": "The Customs office Timetables",
          "items": {
            "$ref": "#/definitions/object_01"
          }
        }
      }
    },
    "object_03": {
      "type": "object",
      "required": [
        "listName",
        "listEntries"
      ],
      "additionalProperties": false,
      "properties": {
        "listName": {
          "type": "string",
          "description": "The name of the list",
          "examples": [
            "CustomsOffices"
          ]
        },
        "listEntries": {
          "anyOf": [
            {
              "type": "array",
              "title": "listEntries",
              "description": "The Custom Office List Entries",
              "items": {
                "$ref": "#/definitions/object_02"
              }
            },
            {
              "type": "array",
              "title": "listEntries",
              "items": {
                "$ref": "#/definitions/object_05"
              }
            }
          ]
        }
      }
    },
    "object_04": {
      "type": "object",
      "required": [
        "variableHolidayDate",
        "day",
        "month"
      ],
      "additionalProperties": false,
      "properties": {
        "variableHolidayDate": {
          "type": "number",
          "minimum": 0,
          "maximum": 1,
          "description": "A flag indicating if the holiday is variable per year",
          "examples": [
            0
          ]
        },
        "day": {
          "type": "string",
          "pattern": "^([0-2][0-9]|3[0-1])$",
          "description": "The day",
          "examples": [
            "31"
          ]
        },
        "month": {
          "type": "string",
          "pattern": "^(0[1-9]|1[0-2])$",
          "description": "The month",
          "examples": [
            "10"
          ]

        },
        "year": {
          "type": "string",
          "pattern": "^[12]\\d{3}$",
          "description": "The year",
          "examples": [
            "1900"
          ]
        },
        "holidayDescriptions": {
          "type": "array",
          "description": "The holiday descriptions in different languages",
          "items": {
            "type": "object",
            "required": [
              "languageCode",
              "publicHolidayName"
            ],
            "additionalProperties": false,
            "properties": {
              "languageCode": {
                "type": "string",
                "pattern": "^[A-Z]{2}$",
                "description": "The language code",
                "examples": [
                  "DE"
                ]
              },
              "publicHolidayName": {
                "type": "string",
                "description": "The holiday name",
                "examples": [
                  "Reformationstag"
                ]
              }
            }
          }
        },
        "descriptions": {
          "type": "array",
          "description": "The holiday descriptions",
          "items": {
            "type": "object",
            "required": [
              "languageCode",
              "publicHolidayName"
            ],
            "additionalProperties": false,
            "properties": {
              "languageCode": {
                "type": "string",
                "pattern": "^[A-Z]{2}$",
                "description": "The language code",
                "examples": [
                  "DE"
                ]
              },
              "publicHolidayName": {
                "type": "string",
                "description": "The holiday name/description",
                "examples": [
                  "NEW YEAR"
                ]
              }
            }
          }
        }
      }
    },
    "object_05": {
      "type": "object",
      "required": [
        "state",
        "activeFrom",
        "countryCode"
      ],
      "additionalProperties": false,
      "properties": {
        "state": {
          "type": "string",
          "pattern": "^(valid|invalid)$",
          "description": "Valid or Invalid entry",
          "examples": [
            "valid"
          ]
        },
        "activeFrom": {
          "type": "string",
          "pattern": "^([12]\\d{3}-(0[1-9]|1[0-2])-(0[1-9]|[12]\\d|3[01]))$",
          "description": "The date the entry was active",
          "examples": [
            "2019-01-01"
          ]
        },
        "countryCode": {
          "type": "string",
          "pattern": "^[A-Z]{2}$",
          "description": "The country code",
          "examples": [
            "AD"
          ]
        },
        "countryRegionCode": {
          "type": "string",
          "description": "The Country region code",
          "pattern": "^([A-Z]{1,3}|[0-9]{2}|[0-9]{2}[A-Z])$",
          "examples": [
            "BR"
          ]
        },
        "descriptions": {
          "type": "array",
          "title": "descriptions",
          "description": "The country region descriptions",
          "items": {
            "type": "object",
            "required": [
              "languageCode",
              "countryRegionName"
            ],
            "additionalProperties": false,
            "properties": {
              "languageCode": {
                "type": "string",
                "pattern": "^[A-Z]{2}$",
                "description": "The language code",
                "examples": [
                  "DE"
                ]
              },
              "countryRegionName": {
                "type": "string",
                "description": "The country region name",
                "examples": [
                  "Brandenburg"
                ]
              }
            }
          }
        },
        "holidays": {
          "type": "array",
          "title": "holidays",
          "description": "The Holidays list",
          "items": {
            "$ref": "#/definitions/object_04"
          }
        }
      }
    },
    "object_06": {
      "description": "The Customs Office Lists",
      "type": "object",
      "required": [
        "messageInformation",
        "customsOffices",
        "countryHolidays",
        "countryRegions"
      ],
      "additionalProperties": false,
      "properties": {
        "messageInformation": {
          "title": "messageInformation",
          "description": "The Message Header Information",
          "type": "object",
          "required": [
            "messageID",
            "snapshotDate"
          ],
          "additionalProperties": false,
          "properties": {
            "messageID": {
              "type": "string",
              "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$",
              "description": "The Message ID",
              "examples": [
                "74bd0784-8dc9-4eba-a435-9914ace26995"
              ]
            },
            "snapshotDate": {
              "type": "string",
              "pattern": "^([12]\\d{3}-(0[1-9]|1[0-2])-(0[1-9]|[12]\\d|3[01]))$",
              "description": "The data snapshot date",
              "examples": [
                "2020-07-06"
              ]
            }
          }
        },
        "customsOffices": {
          "$ref": "#/definitions/object_03"
        },
        "countryHolidays": {
          "$ref": "#/definitions/object_03"
        },
        "countryRegions": {
          "$ref": "#/definitions/object_03"
        }
      }
    }
  }
}